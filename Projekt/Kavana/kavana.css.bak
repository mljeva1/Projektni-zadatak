/* Footer */
footer {
	display: block;
}
.fcont {
	box-sizing: border-box;
	flex: 0 0 auto;
	width: 100%;
}
footer p {
	text-align: center;
	padding: 30px 0px;
	color: #F4E8C1;
	font-weight: 300;
	font-family: 'Lucida Console', Monospace;
}
footer a {
	text-decoration: none;
	color: #F4E8C1;
	font-weight: 300;
	transition: all 0.3s ease-out;
}
footer a:hover {
	color: #70A0AF;
}